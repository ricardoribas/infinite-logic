    //     ]
    //   }
    // ]
    // "react/jsx-wrap-multilines": "off",
    // "react/jsx-filename-extension": {
    //   "extensions": [".jsx", ".tsx", ".ts"]
    // },
    // "react/prop-types": "error"
  },
  "globals": {
    "__DEV__": "readonly"
  }

